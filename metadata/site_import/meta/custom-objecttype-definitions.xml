<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="LinkMoneyAuthToken">
        <display-name xml:lang="x-default">Link Money Auth Token</display-name>
        <description xml:lang="x-default">Link Money custom object for auth token storage</description>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="key">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="token">
                <display-name xml:lang="x-default">token</display-name>
                <type>text</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="tokenExpiryTime">
                <display-name xml:lang="x-default">tokenExpiryTime</display-name>
                <type>datetime</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Link Money">
                <display-name xml:lang="x-default">Link Money</display-name>
                <attribute attribute-id="token"/>
                <attribute attribute-id="tokenExpiryTime"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

    <custom-type type-id="LinkMoneyWebhookSubscriptionKey">
        <display-name xml:lang="x-default">Link Money Webhook Subscription Key</display-name>
        <description xml:lang="x-default">Link Money custom object for webhook subscription key</description>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="key">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="subscriptionKey">
                <display-name xml:lang="x-default">Subscription Key</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Link Money">
                <display-name xml:lang="x-default">Link Money</display-name>
                <attribute attribute-id="subscriptionKey"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

</metadata>
